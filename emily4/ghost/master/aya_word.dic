{
	//現在11個
	'マジカル'
	'リリカル'
	'ロジカル'
	'エンバグ'
	'デバッグ'
	'うっへり'
	'ぐんにょり'
	'フランス'
	'オレンジ'
	'シテオク'
	'ロジック'
}

henihohoho : nonoverlap
{
	parallel henihohoho_marunage
	
	_i = 6 //11/2 = 5.5
	_array = IARRAY
	while _i > 0 {
		_i -= 1
		_array ,= henihohoho_ru + '・' + henihohoho_ru
	}
	parallel _array
	
	'ウンゲレ・バンデラ'
	'スパイク・スリップ'
	'クララ・スタンド'
	'ウガガガ・エススピ'
	'ネコミミ・スクミズ'
	'魔女っ娘・火ねずみ'
	'トロール・はえなわ'
	'ヘニホハ・ヘニホヘ'
	'マジック・ミサイル'
	'アネサン・ロッカク'
	--
	'・ヘニホホホー'
}

